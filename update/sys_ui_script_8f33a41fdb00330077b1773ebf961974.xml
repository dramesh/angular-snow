        <name>x_306229_angular_a.angular_app_client_lo</name>
        <script><![CDATA[var app = angular.module("angularapp", []);
app.controller("AngularAppCtrl", function($scope) {
	$scope.myVar = '';
	$scope.i = 0;
	$scope.clearText = function() {
		$scope.myVar = '';
		$scope.i++;
		alert("The myVar value was cleared " + $scope.i + " times by " + g_user.userName + ".");
	};
});]]></script>
